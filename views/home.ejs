
<%- include("partials/header"); -%>
<div class="mt-5">
    <h1 class="titles">Home</h1>
    <div class="container"> 
        <p> <%= startingContent %> </p>
        <%  posts.forEach(function(post){ %>
       <h3><%=post.title%></h3>
       <p>
       <%=post.content.substring(0,80) + "..." %> 
       <a href="/posts/<%=post._id%>">Read More</a>
       </p>
    </div>
</div>
   <% }) %>
<%- include("partials/footer"); -%>
